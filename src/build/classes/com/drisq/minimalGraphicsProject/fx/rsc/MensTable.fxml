<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.VBox?>

<VBox xmlns="http://javafx.com/javafx/17.0.1"
      xmlns:fx="http://javafx.com/fxml/1"
      fx:id="_rootNode"
      prefHeight="400.0"
      prefWidth="600.0"
      fx:controller="com.drisq.minimalGraphicsProject.fx.MensTableController">
   <properties xmlns="http://javafx.com/javafx/8">
      <controller>
         <fx:reference source="controller"/>
      </controller>
   </properties>
   <children>
      <TableView fx:id="_mensTable" prefHeight="200.0" prefWidth="200.0">
        <columns>
            <TableColumn fx:id="_mensItemCode" prefWidth="75.0" text="C1"/>
            <TableColumn fx:id="_mensGender" prefWidth="75.0" text="C2"/>
        </columns>
      </TableView>
   </children>
</VBox>
